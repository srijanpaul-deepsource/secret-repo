{
  "env": {},
  "extends": [],
  "parserOptions": { "ecmaFeatures": { "jsx": true }, "ecmaVersion": 12, "sourceType": "module" },
  "plugins": ["react", "eslint-plugin-prettier"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "args": "after-used",
        "varsIgnorePattern": "^_",
        "caughtErrors": "none",
        "ignoreRestSiblings": true,
        "vars": "all"
      }
    ]
  }
}

